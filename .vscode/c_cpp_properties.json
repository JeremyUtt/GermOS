    "configurations": [
        {
            "name": "GoopOS",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include"
            ],
            "defines": [
                "__STDC_HOSTED__=0",
                "TEXT_MODE"
